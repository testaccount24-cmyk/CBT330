)Body window(60,04)
%Command ===>_zcmd
%
%sort      ===>@1$= object type+
               @2$= group +
)INIT
  .CURSOR = zcmd
  &zcmd   = &z
  &zwinttl= 'CICS CSD sort object summary'
)REINIT
  REFRESH(*)
)PROC
 VER (&ZCMD,LIST,1,2,'TRACE',MSG=CIC0002)
)PNTS
FIELD(ZPS00001) VAR(ZCMD) VAL(1)
FIELD(ZPS00002) VAR(ZCMD) VAL(2)
)END
