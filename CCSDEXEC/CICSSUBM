PROC 2   DSN MBR
         ISREDIT MACRO
         CONTROL NOFLUSH
         SET &SUBDSN = &STR(&DSN.(&MBR))
         SET SYSOUTTRAP=1
         SUBMIT '&SUBDSN'
         SET &SUBRC EQ &STR(&LASTCC)
         SET SYSOUTTRAP=0
         SET &ZEDSMSG = &STR(CICS JOB(S) SUBMITTED)
         SET &ZEDLMSG = &STR(&SYSOUTLINE1)
         ISPEXEC SETMSG MSG(ISRZ000)
         EXIT CODE(0)
